{
  "spec_dir": "dist",
  "spec_files": [
    "../**/tests/*[S]pec.ts",
    "../**/tests/orderSpec/*[S]pec.ts"
  ],
  "helpers": [
    "helpers/**/*.js"
  ],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "stopSpecOnExpectationFailure": false,
  "random": false
}
